{
  "name": "@giotramu/prettier-config",
  "version": "1.1.0",
  "description": "An opinionated Prettier config.",
  "author": "giotramu <https://twitter.com/giotramu>",
  "homepage": "https://github.com/giotramu/prettier-config#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giotramu/prettier-config.git"
  },
  "bugs": {
    "url": "https://github.com/giotramu/prettier-config/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": ".prettierrc.json",
  "files": [
    ".prettierrc.json",
    "CHANGELOG.md"
  ],
  "scripts": {
    "prettify": "prettier -w -u ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "prettier -w -u"
  },
  "peerDependencies": {
    "prettier": "*"
  },
  "devDependencies": {
    "prettier": "*",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1"
  },
  "keywords": [
    "code-styles",
    "formatter",
    "prettier-config",
    "prettier"
  ]
}
